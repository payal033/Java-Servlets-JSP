<%@ taglib prefix="c" uri="http://java.sun.com/jstl/core_rt"   %>

<!DOCTYPE html>
<html>
<head>
<meta charset="ISO-8859-1">
<title>JSTL Demo</title>
</head>
<body>
<c:out value="${10+20 }"></c:out>
<c:set var="score " scope="session" value="${80 }"></c:set>
Before: <c:out value="${score }" ></c:out>
<c:remove var="score"/>
After: <c:out value="${score }" ></c:out>
</body>
</html>